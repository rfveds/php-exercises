{
  "name": "app/project",
  "description": "Project description",
  "type": "project",
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "ext-pdo": "*",
    "vlucas/phpdotenv": "^5.6",
    "psr/container": "^2.0",
    "symfony/mailer": "^6.0",
    "doctrine/dbal": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.3.4"
  },
  "config": {
    "optimize-autoloader": true
  }
}
